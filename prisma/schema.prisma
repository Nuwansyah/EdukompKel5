datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}


model Register {
  id              String    @id @default(uuid())
  name            String
  age             Int
  Address         String
  CourseT         String
}

model Teacher {
  id              String @id @default(uuid())
  name            String
  age             String
  address         String
  position        String
  createdAt       DateTime @default(now())
}
